{:dependencies [[org.clojure/core.match "0.3.0-alpha4"]
                [com.cognitect/transit-cljs "0.8.239"]
                [com.cognitect/transit-clj "0.8.300"]
                [com.lucasbradstreet/cljs-uuid-utils "1.0.2"]
                [org.clojure/data.json "0.2.6"]

                [fast-zip "0.7.0"]
                [maria/friendly "0.1.0-SNAPSHOT"]
                [maria/shapes "0.1.0-SNAPSHOT"]

                [re-view "0.3.32-SNAPSHOT"]
                [re-view-routing "0.1.3"]
                [re-view-prosemirror "0.1.10-SNAPSHOT"]
                [cljs-live "0.2.8-SNAPSHOT"]


                [lark/commands "0.2.0-SNAPSHOT"]
                [lark/value-viewer "0.1.2-SNAPSHOT"]
                [lark/cells "0.1.5-SNAPSHOT"]

                [lark/structure "0.1.4-SNAPSHOT"]
                [lark/editors "0.1.3-SNAPSHOT"]
                [lark/tree "0.1.3-SNAPSHOT"]

                [org.clojure/tools.reader "1.1.0"]]

 :source-paths ["src"
                "../tree/src"
                "../structure/src"
                "../editors/src"
                "../commands/src"
                "../cells/src"]
 :builds
               {:tests
                {:target     :browser
                 :output-dir "test-target/public/js"
                 :asset-path "js"
                 :modules    {:tests {:entries [tests.tests-runner]}}
                 :devtools   {:after-load tests.tests-runner/run-tests}
                 :js-options {:resolve {"react"       {:target :global
                                                       :global "React"}
                                        "react-dom"   {:target :global
                                                       :global "ReactDOM"}
                                        "codemirror"  {:target :global
                                                       :global "CodeMirror"}
                                        "markdown-it" {:target :global
                                                       :global "markdownit"}}}}}}